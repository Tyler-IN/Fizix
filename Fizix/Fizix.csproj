<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="JetBrains.Annotations" Version="2019.1.3"/>
    </ItemGroup>

    <Choose>
        <When Condition="Exists('..\..\CannyFastMath\CannyFastMath\CannyFastMath.csproj')">
            <ItemGroup>
                <ProjectReference Include="..\..\CannyFastMath\CannyFastMath\CannyFastMath.csproj"/>
            </ItemGroup>
        </When>
        <Otherwise>
            <ItemGroup>
                <PackageReference Include="CannyFastMath" Version="1.0"/>
            </ItemGroup>
        </Otherwise>
    </Choose>

</Project>
